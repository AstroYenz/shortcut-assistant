    "react-dom": "^19.1.0",
    "sonner": "2.0.7",
    "tailwind-merge": "^3.2.0",
    "ts-jest": "^29.4.5",
    "tw-animate-css": "^1.2.8",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
